main <- function(){
acc <- 0
min <- 0
max <- 0.8
iteracoes <- 100
incremento <- (max - min)/iteracoes
acc <- acc + f(min)
for(i in seq(from=(min+incremento), to=max, by=incremento)){
if(i == max){
acc <- acc + f(i)
}else{
acc <- acc + 2 * f(i)
}
}
acc <- acc / (2 * iteracoes)
acc <- acc * max - min
cat(acc)
}
